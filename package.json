{
  "name": "hyperswarm-hugin",
  "version": "4.16.0-hugin",
  "description": "A distributed networking stack for connecting peers with added connection security for Hugin",
  "files": [
    "index.js",
    "lib/**.js"
  ],
  "imports": {
    "events": {
      "bare": "bare-events",
      "default": "events"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    }
  },
  "dependencies": {
    "b4a": "^1.3.1",
    "bare-events": "^2.2.0",
    "hyperdht-hugin": "^1.0.3",
    "safety-catch": "^1.0.2",
    "shuffled-priority-queue": "^2.1.0",
    "streamx": "^2.22.1",
    "unslab": "^1.3.0"
  },
  "devDependencies": {
    "bare-process": "^4.2.2",
    "brittle": "^3.0.2",
    "hypercore-crypto": "^3.4.0",
    "lunte": "^1.3.0",
    "prettier": "^3.6.2",
    "prettier-config-holepunch": "^2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check . && lunte",
    "test": "npm run test:node && npm run test:bare",
    "test:generate": "brittle -r test/all.js test/*.js",
    "test:node": "brittle-node --coverage test/*.js",
    "test:bare": "brittle-bare --coverage test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/n9lsjr/hyperswarm-hugin.git"
  },
  "author": "n9lsjr",
  "contributors": [
    "Mathias Buus (@mafintosh)",
    "David Mark Clements (@davidmarkclem)",
    "Andrew Osheroff (@andrewosh)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n9lsjr/hyperswarm-hugin/issues"
  },
  "homepage": "https://github.com/n9lsjr/hyperswarm-hugin"
}
